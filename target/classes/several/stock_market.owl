<?xml version="1.0"?>
<rdf:RDF xmlns="https://dcm.ffclrp.usp.br/lssb/stock-market-ontology#"     
     xml:base="https://dcm.ffclrp.usp.br/lssb/stock-market-ontology#"    
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:stock="https://dcm.ffclrp.usp.br/lssb/stock-market-ontology#"> 

    <!-- Declaração da Ontologia -->
    <owl:Ontology rdf:about="https://dcm.ffclrp.usp.br/lssb/stock-market-ontology#">
        <owl:versionInfo>2.0.4</owl:versionInfo> <!-- Propriedade padrão para versão -->
        <rdfs:comment>Ontologia sobre o mercado de ações.</rdfs:comment>
        <!-- Se você tiver imports, eles iriam aqui: <owl:imports rdf:resource="..."/> -->
    </owl:Ontology>

    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- Não precisamos declarar rdfs:comment, já é padrão -->


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    // Definidas usando owl:ObjectProperty e rdf:about="#nomePropriedade"
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="#atuaEm">
        <rdfs:domain rdf:resource="#Empresa"/>
        <rdfs:range rdf:resource="#Setor_Atuacao"/>
        <rdfs:comment>Propriedade que indica que uma empresa atua em um determinado setor econômico.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#defineIndicador">
        <rdfs:domain rdf:resource="#Bolsa_de_Valores"/>
        <rdfs:range rdf:resource="#Indice"/>
        <rdfs:comment>Propriedade que indica que uma bolsa de valores define um indicador formado por um conjunto teórico de valores mobiliários.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#eListada">
        <rdfs:domain rdf:resource="#Empresa_Capital_Aberto"/>
        <rdfs:range rdf:resource="#Bolsa_de_Valores"/>
        <rdfs:comment>Propriedade que indica que os valores mobiliários de uma empresa de capital aberto são negociados em uma bolsa de valores.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#negociado">
        <rdfs:domain rdf:resource="#Valor_Mobiliario_Negociado"/>
        <rdfs:range rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:comment>Propriedade que indica que um valor mobiliário é negociado em um determinado pregão.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#negociadoDurante">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="#Pregao"/>
        <rdfs:comment>Pregão no qual os valores mobiliários são negociados.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#negociadoPregoes">
        <rdfs:domain rdf:resource="#Valor_Mobiliario_Negociado"/>
        <rdfs:range rdf:resource="#Negociado_Em_Diferentes_Pregoes"/>
        <rdfs:comment>Propriedade que indica que um mesmo valor mobiliário é negociado em diferentes pregões.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#organiza">
        <rdfs:domain rdf:resource="#Bolsa_de_Valores"/>
        <rdfs:range rdf:resource="#Pregao"/>
        <rdfs:comment>Propriedade que indica que uma bolsa de valores organiza pregão, onde ocorre a negociação de valores mobiliários.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#pregaoBase">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Negociado_Em_Diferentes_Pregoes"/>
        <rdfs:range rdf:resource="#Pregao"/>
        <rdfs:comment>Propriedade que indica que um valor mobiliario negociado em diferentes pregões tem um pregão base.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#pregaoPosterior">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Negociado_Em_Diferentes_Pregoes"/>
        <rdfs:range rdf:resource="#Pregao"/>
        <rdfs:comment>Propriedade que indica que um valor mobiliario negociado em diferentes pregões tem um pregão posterior.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#representaConjuntoDe">
        <rdfs:domain rdf:resource="#Certificado_de_Deposito_de_Acoes"/>
        <rdfs:range rdf:resource="#Acao"/>
        <rdfs:comment>Propriedade que indica que um certificado de depósito de ações representa um determinado número de ações de uma empresa listada na bolsa de valores.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#representadoPor">
        <rdfs:domain rdf:resource="#Valor_Mobiliario_Negociado"/>
        <rdfs:range rdf:resource="#Codigo_Negociacao"/>
        <rdfs:comment>Propriedade que indica que um determinado valor mobiliário negociado em um pregão é representado por um código de negociação.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#temIdentificacao">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Valor_Mobiliario"/>
        <rdfs:range rdf:resource="#Codigo_Negociacao"/>
        <rdfs:comment>Propriedade que indica que um valor mobiliário é identificado por um código de negociação.</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#temValorMobiliarioNegociado">
        <rdfs:domain rdf:resource="#Empresa_Capital_Aberto"/>
        <rdfs:range rdf:resource="#Valor_Mobiliario_Negociado"/>
        <rdfs:comment>Relacionamento que indica que uma empresa de capital aberto tem seus valores mobiliários negociados em uma bolsa de valores.</rdfs:comment>
    </owl:ObjectProperty>

    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data Properties
    // Definidas usando owl:DatatypeProperty
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="#horarioAbertura">
        <rdfs:domain rdf:resource="#Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Horário de abertura de um pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#horarioFechamento">
        <rdfs:domain rdf:resource="#Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Horário de fechamento de um pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#intervaloIntradiarioAbsoluto">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação intradiária entre os preços máximo e mínimo de um valor mobiliário negociado em um pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#intervaloIntradiarioPercentual">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação intradiária percentual ou relativa entre os preços máximo e mínimo de um valor mobiliário negociado em um pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#ocorreEmData">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Pregao"/>
        <!-- O range pode ser date ou dateTime. Se usar URI para data, seria ObjectProperty -->
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/> <!-- Usando xsd:date -->
        <rdfs:comment>Data na qual um pregão ocorre.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#precoAbertura">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Preço utilizado como base para o início das negociações do valor mobiliário no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#precoFechamento">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Preço do valor mobiliário associado ao último negógio no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#precoMaximo">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Maior preço negociado pelo valor mobiliário no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#precoMedio">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/> <!-- Double parece mais apropriado que positiveInteger -->
        <rdfs:comment>Preço médio negociado pelo valor mobilário no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#precoMinimo">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Menor preço negociado pelo valor mobilário no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#ticker">
        <rdfs:domain rdf:resource="#Codigo_Negociacao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#token"/>
        <rdfs:comment>Conjunto de letras e números que identifica um valor mobiliário negociado em uma bolsa de valores.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#totalNegocios">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment>Quantidade total de negócios associados ao valor mobiliário no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#totalTitulosNegociados">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment>Quantidade total de títulos do valor mobiliários negociados no pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#variacaoAbsoluta">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Negociado_Em_Diferentes_Pregoes"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação absoluta no preço de um valor mobiliário negociado em diferentes pregões.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#variacaoIntradiariaAbsoluta">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação absoluta no preço de um valor mobiliário negociado, ocorrida durante um mesmo pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#variacaoIntradiariaPercentual">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação percentual ou relativa no preço de um valor mobiliário negociado, ocorrida durante um mesmo pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#variacaoPercentual">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Negociado_Em_Diferentes_Pregoes"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Variação percentual ou relativa no preço de um valor mobiliário negociado em diferentes pregões.</rdfs:comment>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#volumeNegociacao">
        <rdfs:domain rdf:resource="#Negociado_Em_Pregao"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Volume total do valor mobiliário negociado durante o pregão.</rdfs:comment>
    </owl:DatatypeProperty>

    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    // Definidas usando owl:Class e rdf:about="#nomeClasse"
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

     <!-- Declaração da Classe Base (exemplo) -->
     <owl:Class rdf:about="#Setor_Atuacao">
        <rdfs:comment>Setor_Atuacao representa um setor econômico em que uma empresa pode atuar.</rdfs:comment>
     </owl:Class>

     <!-- Exemplo de Subclasse -->
     <owl:Class rdf:about="#Bens_Industriais">
          <rdfs:subClassOf rdf:resource="#Setor_Atuacao"/>
          <rdfs:comment>Setor econômico que representa a produção, comercialização, construção, transporte e serviços diversos relacionados a bens industriais.</rdfs:comment>
     </owl:Class>

    <owl:Class rdf:about="#Acao"> <rdfs:subClassOf rdf:resource="#Valor_Mobiliario"/> <rdfs:comment>Acao representa a parte do capital de uma empresa.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Acessorios"> <rdfs:subClassOf rdf:resource="#Tecidos._Vestuarios_e_Calcados"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de artigos de cuidados pessoais, cutelaria, jóias, relógios e outros acessórios.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Acucar_e_Alcool"> <rdfs:subClassOf rdf:resource="#Alimentos_Processados"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de açúcar, álcool, bioenergia e outros derivados.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Aereo"> <rdfs:subClassOf rdf:resource="#Transporte"/> <rdfs:comment>Setor econômico que representa a prestação de serviços de transporte e logística aéreos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Agricultura"> <rdfs:subClassOf rdf:resource="#Agropecuaria"/> <rdfs:comment>Setor econômico que representa a comercialização de insumos agrícolas, semestes, cereais, grãos e frutas.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Agropecuaria"> <rdfs:subClassOf rdf:resource="#Consumo_Nao_Ciclico"/> <rdfs:comment>Produção e comercialização de algodão, sementes, grãos, cereais e frutas, e comercialização de fertilizantes, defensivos biológicos e inoculantes.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Agua_e_Saneamento"> <rdfs:subClassOf rdf:resource="#Utilidade_Publica"/> <rdfs:comment>Setor econômico que representa a prestação de serviços públicos envolvendo o abastecimento de água potável, coleta e tratamento de esgoto e gestão de resíduos sólidos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Alimentos"> <rdfs:subClassOf rdf:resource="#Comercio_e_Distribuicao"/> <rdfs:comment>Setor econômico que representa a comercialização e a distribuição de alimentos, produtos de limpeza e higiene.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Alimentos_Diversos"> <rdfs:subClassOf rdf:resource="#Alimentos_Processados"/> <rdfs:comment>Setor econômico que representa a industrialização e comercialização de grãos, pescados, açúcar, massas, biscoitos, café e outros produtos derivados de trigo.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Alimentos_Processados"> <rdfs:subClassOf rdf:resource="#Consumo_Nao_Ciclico"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Aluguel_de_Carros"> <rdfs:subClassOf rdf:resource="#Diversos"/> <rdfs:comment>Setor econômico que representa serviços de locação de veículos e gestão de frotas.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Armas_e_Municoes"> <rdfs:subClassOf rdf:resource="#Maquinas_e_Equipamentos"/> <rdfs:comment>Setor econômico que representa a produção e comercialização de armas e munições.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Artefatos_de_Cobre"> <rdfs:subClassOf rdf:resource="#Siderurgia_e_Metalurgia"/> <rdfs:comment>Setor econômico que representa a metalurgia do cobre e suas ligas.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Artefatos_de_Ferro_e_Aco"> <rdfs:subClassOf rdf:resource="#Siderurgia_e_Metalurgia"/> <rdfs:comment>Setor econômico que representa a industrialização e comercialização de aços planos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Atividades_Esportivas"> <rdfs:subClassOf rdf:resource="#Viagens_e_Lazer"/> <rdfs:comment>Setor econômico que representa a prestação de serviços de condicionamento físico.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Automoveis_e_Motocicletas"> <rdfs:subClassOf rdf:resource="#Consumo_Ciclico"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de de peças e componentes do setores automotivo e ferroviário.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Bancos"> <rdfs:subClassOf rdf:resource="#Intermediarios_Financeiros"/> <rdfs:comment>Setor econômico que representa a comercialização de produtos e serviços bancários.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Bebidas"> <rdfs:subClassOf rdf:resource="#Consumo_Nao_Ciclico"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
    <!-- Bens_Industriais já definida acima -->
    <owl:Class rdf:about="#Bicicletas"> <rdfs:subClassOf rdf:resource="#Viagens_e_Lazer"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de bicicletas.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Bolsa_de_Valores"> <rdfs:comment>Bolsa de valores é uma organização que permite a negociação de valores mobiliários.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Brazilian_Depositary_Receipt"> <rdfs:subClassOf rdf:resource="#Valor_Mobiliario"/> <rdfs:comment>Brazilian_Depositary _Receipt é um certificado de depósito de ações emitido no Brasil que representa ações de empresas estrangeiras sem registro no Brasil.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Brinquedos_e_Jogos"> <rdfs:subClassOf rdf:resource="#Viagens_e_Lazer"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de brinquedos e jogos recreativos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Calcados"> <rdfs:subClassOf rdf:resource="#Tecidos._Vestuarios_e_Calcados"/> <rdfs:comment>Setor econômico que representa a fabricação e comercialização de calçados e artigos esportivos e de viagem.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Carnes_e_Derivados"> <rdfs:subClassOf rdf:resource="#Alimentos_Processados"/> <rdfs:comment>Setor econômico que representa a produção e distribuição de alimentos à base de carne bovina, suína, ovina e avícola.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Certificado_de_Deposito_de_Acoes"> <rdfs:subClassOf rdf:resource="#Valor_Mobiliario"/> <rdfs:comment>Certificado_de_Deposito_de_Acoes representa a propriedade de um conjunto de ações ordinárias e/ou preferenciais de uma empresa.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Cervejas_e_Refrigerantes"> <rdfs:subClassOf rdf:resource="#Bebidas"/> <rdfs:comment>Setor econômico que representa a fabricação e distribuição de cervejas, refrigerantes e outras bebidas não carbonatadas e não alcoólicas.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Classe_A"> <rdfs:subClassOf rdf:resource="#Preferencial"/> <rdfs:comment>Ação cujo acionista recebe um mínimo de dividendos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Classe_B"> <rdfs:subClassOf rdf:resource="#Preferencial"/> <rdfs:comment>Ação cujo acionista recebe um valor pré-definido de dividendo.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Codigo_Negociacao"> <rdfs:comment>Código de negociação é um símbolo que identifica o título de valor mobiliário.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comercio"> <rdfs:subClassOf rdf:resource="#Consumo_Ciclico"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comercio_de_Eletrodomesticos"> <rdfs:subClassOf rdf:resource="#Comercio"/> <rdfs:comment>Setor econômico que representa a fabricação de eletrodomésticos e comercialização de móveis, eletrodomésticos e eletrônicos.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comercio_de_Materiais_de_Transporte"> <rdfs:subClassOf rdf:resource="#Bens_Industriais"/> <rdfs:comment>Setor econômico que representa o comércio de caminhões, ônibus, automóveis, camionetas, acessórios, máquinas e outros equipamentos de transporte.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comercio_e_Distribuicao"> <rdfs:subClassOf rdf:resource="#Consumo_Nao_Ciclico"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comercio_e_Distribuicao_de_Medicamentos_e_Outros_Produtos"> <rdfs:subClassOf rdf:resource="#Saude"/> <rdfs:comment>Setor econômico que representa a produção, comercialização e distribuição de medicamensos e de outros produtos médico-hospitalares e laboratoriais para a área da saúde.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Computadores_e_Equipamentos"> <rdfs:subClassOf rdf:resource="#Tecnologia_da_Informacao"/> <rdfs:comment>Setor econômico que representa a fabricação de equipamentos de informática.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Comunicacoes"> <rdfs:subClassOf rdf:resource="#Setor_Atuacao"/> <rdfs:comment>Setor econômico que representa a prestação de serviços de telecomunicação em geral e publicidade.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Construcao_Civil"> <rdfs:subClassOf rdf:resource="#Consumo_Ciclico"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Construcao_Pesada"> <rdfs:subClassOf rdf:resource="#Construcao_e_Engenharia"/> <rdfs:comment>Setor econômico que representa a execução de obras de construção pesada.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Construcao_e_Agricolas"> <rdfs:subClassOf rdf:resource="#Maquinas_e_Equipamentos"/> <rdfs:comment>Setor econômico que representa a fabricação de máquinas, equipamentos, peças e acessórios para a agricultura e pecuária.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="#Construcao_e_Engenharia"> <rdfs:subClassOf rdf:resource="#Bens_Industriais"/> <rdfs:comment xml:lang="pt"></rdfs:comment> </owl:Class>
